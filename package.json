{
  "name": "bip38-network",
  "version": "3.0.0",
  "description": "BIP38 is a standard process to encrypt Bitcoin and crypto currency private keys that is impervious to brute force attacks thus protecting the user.",
  "main": "index.js",
  "keywords": [
    "bitcoin",
    "crypto",
    "cryptography",
    "litecoin"
  ],
  "homepage": "https://github.com/sondreb/bip38",
  "author": "JP Richardson",
  "dependencies": {
    "bigi": "1.4.2",
    "browserify-aes": "1.2.0",
    "bs58check": "2.1.2",
    "buffer-xor": "2.0.2",
    "create-hash": "1.2.0",
    "ecurve": "1.0.6",
    "scrypt-async": "2.0.1"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "mochify": "6.0.4",
    "standard": "12.0.1",
    "wif": "2.0.6"
  },
  "repository": {
    "url": "git@github.com:sondreb/bip38.git",
    "type": "git"
  },
  "scripts": {
    "browser-test": "mochify --wd -R spec --timeout 100000",
    "coverage": "istanbul cover _mocha -- --reporter list test/*.js",
